{
    "useTabs": true,
    "printWidth": 80,
    "tabWidth": 2,
    "singleQuote": true,
    "trailingComma": "none",
    "jsxBracketSameLine": false,
    "parser": "flow",
    "semi": false,
    "editor.formatOnPaste": false,
    "editor.formatOnSave": true,
    "[javascript]": {
    "editor.formatOnSave": true
    },
    "[html]": {
    "editor.formatOnSave": false
    },
    "[json]": {
    "editor.formatOnSave": false
    },
    "eslint.autoFixOnSave": true,
    "eslint.alwaysShowStatus": true
}